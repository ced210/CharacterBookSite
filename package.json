{
  "name": "character-book",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=developpement vue-cli-service serve",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "start": "node server/server.js",
    "start:build": "cross-env NODE_ENV=production node server/server.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.3.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "material-design-icons-iconfont": "^5.0.1",
    "prettier": "^1.19.1",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "vue/no-unused-components": "warn",
      "vue/no-unused-vars": "warn",
      "vue/no-use-v-if-with-v-for": "warn",
      "vue/require-component-is": "warn",
      "vue/require-prop-type-constructor": "warn",
      "vue/require-v-for-key": "warn",
      "vue/require-valid-default-prop": "warn",
      "vue/valid-v-else-if": "warn",
      "vue/valid-v-else": "warn",
      "vue/valid-v-for": "warn",
      "vue/valid-v-html": "warn",
      "vue/valid-v-if": "warn",
      "vue/valid-v-model": "warn",
      "vue/valid-v-on": "warn",
      "vue/valid-v-once": "warn",
      "vue/attribute-hyphenation": "warn",
      "vue/html-closing-bracket-spacing": "warn",
      "vue/html-quotes": "warn",
      "vue/html-self-closing": "warn",
      "vue/no-multi-spaces": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
